#: src/module/icmp/state.cc:42
msgid "${name} address is invalid"
msgstr "Endereço de ${name} e inválido"

#: src/module/icmp/state.cc:49
msgid "${name} is active"
msgstr "${name} está ativo"

#: src/module/icmp/state.cc:63
msgid "${name} is not acessible"
msgstr "${name} não está acessível"

#: src/module/icmp/state.cc:70
msgid "${name} is not available"
msgstr "${name} não está disponível"

#: src/module/icmp/state.cc:56
msgid "${name} is not reachable"
msgstr "Não foi possível alcançar ${name}"

#: src/library/os/linux/defaultgateway.cc:63
msgid "Cant get netlink socket"
msgstr "Não foi possível obter o socket netlink"

#: src/library/os/linux/defaultgateway.cc:101
msgid "Cant receive netlink response"
msgstr "Não foi possível receber resposta netlink"

#: src/library/os/linux/defaultgateway.cc:87
msgid "Cant send netlink message"
msgstr "Não foi possível enviar dados netlink"

#: src/module/icmp/state.cc:57
msgid ""
"Destination Unreachable. The gateway doesnt know how to get to the defined "
"network."
msgstr ""
"Destino indisponível. O roteador não sabe como chegar até a rede definida."

#: src/library/os/linux/defaultgateway.cc:108
msgid "Error in received packet"
msgstr "Erro no pacote recebido"

#: src/module/icmp/state.cc:50
msgid "Got ICMP echo reply from host."
msgstr "Obtive resposta ICMP do host"

#: src/library/os/linux/defaultgateway.cc:158
msgid "Invalid size on RTA_GATEWAY"
msgstr "Tamanho inválido em RTA_GATEWAY"

#: src/module/icmp/state.cc:71
msgid "No ICMP response from host."
msgstr "Nenhuma resposta ICMP do host."

#: src/module/icmp/state.cc:64
msgid ""
"Time Exceeded. The ICMP request has been discarded because it was 'out of "
"time'."
msgstr ""
"Tempo excedido. A requisição ICMP foi descartada porque estava 'fora de "
"tempo'."

#: src/module/icmp/state.cc:43
msgid "Unable to get a valid IP address for ${name}."
msgstr "Incapaz de obter um endereço IP válido para ${name}"
